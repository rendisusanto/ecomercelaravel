{
    "version": 2,
    "framework": null,
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "installCommand": "npm install",
    "devCommand": "npm run dev",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}